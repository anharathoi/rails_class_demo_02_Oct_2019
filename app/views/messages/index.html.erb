<h1>Contact me</h1>

<div class="contact-form">
  <%= form_with model: @message do |f| %>
  <div>
    <%= f.label :name %>:<br>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :email %>:<br>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :message_body %>: <br>
    <%= f.text_area :message_body %>
  </div>

    <%= f.submit %>
  <% end %>
</div>
